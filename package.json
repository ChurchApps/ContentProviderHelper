{
  "name": "@churchapps/content-provider-helper",
  "version": "0.0.9",
  "type": "module",
  "description": "Helper classes for interacting with third party providers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vite",
    "cli": "npx tsx cli/playground.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "keywords": [
    "churchapps",
    "content-provider",
    "helper"
  ],
  "author": "ChurchApps",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChurchApps/ContentProviderHelper.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.0.0",
    "cli-table3": "^0.6.5",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.2.0",
    "ora": "^8.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.35.0",
    "vite": "^5.0.0"
  }
}
